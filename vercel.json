{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "public": true,
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true,
      "preview": false
    }
  },
  "environments": {
    "production": {
      "source": ["main"]
    },
    "preview": {
      "source": ["dev"]
    }
  }
}